
! Word-hopping
URxvt.keysym.Control-Left: \033[1;5D
URxvt.keysym.Control-Right: \033[1;5C
URxvt.keysym.Alt-Left: \033[1;5D
URxvt.keysym.Alt-Right: \033[1;5C




aXft.dpi:                       96
URxvt*termName:                 urxvt-terminal
!URxvt*font:                     xft:JetBrainsmono:pixelsize=12:antialias=true
!URxvt.boldfont:                 xft:JetBrainsMono-Bold:size=12:antialias=true
URxvt*lineSpace:                1
URxvt.letterSpace:              -1
URxvt.iso14755:                 false
URxvt.iso14755_52:              false
URxvt*geometry:                 132x43
URxvt.scrollBar:                false
URxvt*loginShell:               true
URxvt.internalBorder:           2
URxvt*scrollTtyOutput:          true
Xft.antialias:  true

! fonts
URxvt.font:     xft:JetBrainsmono:size=12:antialias=true
URxvt.boldFont: xft:JetBrainsMono-Bold:size=12:antialias=true

! transparency TODO
!  URxvt.transparent:   true
!  URxvt*transparent:   true
!  URxvt*depth:         32
!  URxvt.shading:       20

! -----perl-externals-----
URxvt.perl-ext-common:          clipboard,tabbed,matcher,default
! TODO: font stuff

! perl-clipboard
URxvt.keysym.Shift-Control-C:   perl:clipboard:copy
URxvt.keysym.Shift-Control-V:   perl:clipboard:paste
URxvt.clipboard.autocopy:       true

! perl-tabbed
URxvt.tabbed.tabbar-fg: 2
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 3
URxvt.tabbed.tab-bg: 0

! perl-resize-font
! URxvt.keysym.Control-UP:      resize-font:smaller
! URxvt.keysym.Control-DOWN:    resize-font:bigger

! perl-URL-launcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

! colors
*.foreground:   #F5F5F5
*.background:   #080808
*.cursorColor:  #fff

! black
*.color0:       #1d1f21
*.color8:       #969896

! red
*.color1:       #cc6666
*.color9:       #cc6666

! green
*.color2:       #b5bd68
*.color10:      #b5bd68

! yellow
*.color3:       #f0c674
*.color11:      #f0c674

! blue
*.color4:       #3d62f5
*.color12:      #3d62f5

! magenta
*.color5:       #b294bb
*.color13:      #b294bb

! cyan
*.color6:       #8abeb7
*.color14:      #8abeb7

! white
*.color7:       #c5c8c6
*.color15:      #ffffff
